{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/index.js"
      }
    ],
    "builds": [
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/build/**/*",
        "use": "@vercel/static"
      }
    ]
  }
  